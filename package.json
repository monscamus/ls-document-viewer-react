{
  "name": "legalesign-document-viewer-react",
  "version": "0.1.3",
  "description": "Legalesign React Editor / Viewers",
  "keywords": [
    "stencilJS",
    "React",
    "components",
    "Legalesign",
    "ui"
  ],
  "author": "Alex <alex.weinle@legalesign.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "types": "./loader/index.d.ts",
      "require": "./loader/index.cjs"
    }
  },
  "include": [
    "lib"
  ],
  "exclude": [
    "node_modules"
  ],
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p . --outDir ./dist",
    "getiton": "pnpm install legalesign-document-viewer@latest",
    "publish": "npm publish --scope=public"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "overrides": {
    "@tanstack/react-virtual": "3.9.0"
  },
  "dependencies": {
    "@stencil/react-output-target": "^1.2.0",
    "legalesign-document-viewer": "^0.1.3"
  },
  "pnpm": {
    "overrides": {
      "legalesign-document-viewer": "link:../../../Library/pnpm/global/5/node_modules/legalesign-document-viewer"
    }
  }
}
